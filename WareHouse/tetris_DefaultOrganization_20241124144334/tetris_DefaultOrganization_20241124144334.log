class Piece:
    def __init__(self):
        self.x = 0
        self.y = 0
        self.shape = [[0] * 4 for _ in range(4)]
    def move(self, dx, dy):
        self.x += dx
        self.y += dy
    def rotate(self):
        self.shape = self.get_rotated_shape()
    def get_rotated_shape(self):
        n = len(self.shape)
        m = len(self.shape[0])
        rotated_shape = [[0] * n for _ in range(m)]
        for i in range(n):
            for j in range(m):
                rotated_shape[j][n - i - 1] = self.shape[i][j]
        return rotated_shape
